{
  "type": "module",
  "version": "1.0.0",
  "module": "commonjs",
  "scripts": {
    "sW": "concurrently \"tsc --watch\" \"nodemon --delay 1s nodejs/jaksecfileserver.js\" \"nodemon nodejs/jaksecserver.js\"",
    "notscsW": "concurrently  \"nodemon --delay 1s nodejs/jaksecfileserver.js\" \"nodemon --delay 1s nodejs/jaksecserver.js\"",
    "test": "jest",
    "all": "npm i && concurrently \"tsc --watch\" \"nodemon --delay 1s nodejs/jaksecfileserver.js\" \"nodemon --delay 1s nodejs/jaksecserver.js\" \"cd ../frontend && npm run all\"",
    "alldev": "npm i && concurrently \"tsc --watch\" \"nodemon --delay 1s nodejs/jaksecfileserver.js\" \"nodemon --delay 1s nodejs/jaksecserver.js\" \"cd ../frontend && npm run alldev\"",
    "tsc": "tsc",
    "tscW": "tsc --watch",
    "docs": "typedoc src",
    "docs:watch": "nodemon --watch 'src/**/*.ts' --exec npm run docs",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx frontend/src backend/src",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx frontend/src backend/src"
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "docdash": "^2.0.2",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-validator": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mock-socket": "^9.3.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.3",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.7",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^9.5.0",
    "rotating-file-stream": "^3.2.5",
    "socket.io": "^4.8.0",
    "typedoc-material-theme": "^1.1.0",
    "typescript": "^5.6.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.9",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.0.1",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.4",
    "socket.io-mock": "^1.3.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "typedoc": "^0.26.10",
    "typedoc-comments": "^1.0.1",
    "typedoc-plugin-coverage": "^3.3.0",
    "typedoc-plugin-extras": "^3.1.0",
    "typedoc-plugin-markdown": "^4.2.9"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
